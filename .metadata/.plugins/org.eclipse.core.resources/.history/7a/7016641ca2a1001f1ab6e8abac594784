package com.eshopping.profile.service;

import com.eshopping.profile.entity.UserProfile;

public interface UserService {
	UserProfile addUserProfile(UserProfile userProfile);

	UserProfile getUserProfile(Long profileId);

	UserProfile getUserProfileByMobile(String mobileNumber);

	UserProfile getUserProfileByName(String firstName, String lastName);

	void deleteUserProfile(Long profileId);
}
